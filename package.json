    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "chai": "4.2.0",
    "eslint": "7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "husky": "4.3.6",
    "lodash.range": "3.2.0",
    "mocha": "8.2.1",
    "mysql2": "2.2.5",
    "nyc": "15.1.0",
    "nyc-config-common": "1.0.1",
    "prettier": "^2.2.1",
    "promise-sequential": "1.1.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "^3.0.2",
    "semantic-release": "17.3.1",
