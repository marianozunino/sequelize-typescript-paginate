    "lint": "eslint . --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "build": "npm run clean && tsc",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' TS_NODE_PROJECT=tsconfig.spec.json nyc mocha --exit --no-timeouts -r ts-node/register 'test/**/*.ts'"
